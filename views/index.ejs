<div class="author">
  <h1 class="author__heading">Search Results</h1>
</div>

<section class="articles">

  <ul class="article-ul">
    <% data.forEach(post => { %>
      <li>
        <a href="/post/<%= post._id %>">
          <span><%= post.title %></span>
          <span class="article-list__date">
  <%= post.createdAt ? post.createdAt.toDateString() : "No Date" %>
</span>

        </a>
      </li>
    <% }) %>
  </ul>
<% if (nextPage!=null){%>
  <a href = "/?page=<%=nextPage%>" class="pagination">&lt;View Older post</a>
  <%
}%>

</section>